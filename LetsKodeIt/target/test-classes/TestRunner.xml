<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Facebook" parallel="tests">

    <test name="Lets kode it Homepages">
        <parameter name="useCloudEnv" value="false"/>
        <parameter name="cloudEnvName" value="browserstack"/>
        <parameter name="os" value="win"/>
        <parameter name="os_version" value="Sierra"/>
        <parameter name="browser_name" value="chrome"/>
        <parameter name="browser_version" value="53.0"/>
        <parameter name="url" value="http://letskodeit.teachable.com/"/>
        <parameter name="pathForReports" value="../LetsKodeIt/reports/test"/>
        <parameter name="path" value="../LetsKodeIt/screenshots/"/>
        <classes>
            <class name="testPageObjects.TestHomePages"/>
            <!--<class name="testPageObjects.TestPracticePages"/>-->
        </classes>
    </test>

</suite>